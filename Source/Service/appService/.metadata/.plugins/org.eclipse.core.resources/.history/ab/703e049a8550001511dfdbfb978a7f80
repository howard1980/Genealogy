package com.maxiaohua.genealogy.main.db.view.support;

import com.maxiaohua.genealogy.main.db.base.BaseTableDTO;
import com.maxiaohua.genealogy.main.db.view.VWorkerAccountDTO;

/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class VWorkerAccountDTOSupport extends BaseTableDTO {

	public static final String EQUAL_SEARCH = "VWORKER_ACCOUNT_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "VWORKER_ACCOUNT_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "VWORKER_ACCOUNT_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "VWORKER_ACCOUNT_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "VWORKER_ACCOUNT_SQL_SEARCH";

	public static final String COUNT = "VWORKER_ACCOUNT_COUNT";

	public static final String COUNT_OR = "VWORKER_ACCOUNT_COUNT_OR";

	public static final String COUNT_SEARCH = "VWORKER_ACCOUNT_COUNT_SEARCH";

	public static final String COLUMN_WORKER_GROUP_ID = "WORKER_GROUP_ID";

	public static final String COLUMN_TOTEL_ACCOUNT = "TOTEL_ACCOUNT";

	public static final String COLUMN_TODAY_ACCOUNT = "TODAY_ACCOUNT";

	public static final String TABLE = "VWORKER_ACCOUNT";

	public static VWorkerAccountDTO setDefaultValue() {
		VWorkerAccountDTO defaultDTO = new VWorkerAccountDTO();

		defaultDTO.setWorkerGroupID(0);

		defaultDTO.setTotelAccount(new java.math.BigDecimal(0));

		defaultDTO.setTODAYAccount(new java.math.BigDecimal(0));

		return defaultDTO;
	}
}
