  <select id="T02_PERSONAL_LIKE_SEARCH" resultType="com.maxiaohua.genealogy.main.db.tbl.T02PersonalDTO">
    SELECT
      ID AS "iD" ,
      FIRST_NAME AS "fIRSTNAME" ,
      LAST_NAME AS "lASTNAME" ,
      NO AS "nO" ,
      MOBILE AS "mOBILE" ,
      MAILE AS "mAILE" ,
      BIRTH_DATE AS "bIRTHDATE" ,
      SEX AS "sEX" ,
      COVER AS "cOVER" ,
      HEAD_PORTRAIT AS "hEADPORTRAIT" ,
      VIDEO AS "vIDEO" ,
