  <!-- Table Insert -->
  <insert id="T02_PERSONAL_INSERT" parameterType="com.maxiaohua.genealogy.main.db.tbl.T02PersonalDTO">
    INSERT INTO T02_PERSONAL (
      FIRST_NAME ,
      LAST_NAME ,
      NO ,
      MOBILE ,
      MAILE ,
      BIRTH_DATE ,
      SEX ,
      COVER ,
      HEAD_PORTRAIT ,
      VIDEO ,
      INFOMATION ,
      LATITUDE ,
      LONGITUDE ,
      REGEDIT_TIME 
    ) VALUES (
      #{fIRSTNAME} ,
      #{lASTNAME} ,
      #{nO} ,
      #{mOBILE} ,
      #{mAILE} ,
      #{bIRTHDATE} ,
      #{sEX} ,
      #{cOVER} ,
      #{hEADPORTRAIT} ,
      #{vIDEO} ,
      #{iNFOMATION} ,
      #{lATITUDE} ,
      #{lONGITUDE} ,
      #{rEGEDITTIME} 
    )
    <selectKey resultType="java.lang.Long" order="AFTER" keyProperty = "iD">
    SELECT LAST_INSERT_ID() AS ID
    </selectKey>
  </insert>

  <!-- Table Update -->
  <update id="T02_PERSONAL_FULL_UPDATE" parameterType="com.maxiaohua.genealogy.main.db.tbl.T02PersonalDTO">
