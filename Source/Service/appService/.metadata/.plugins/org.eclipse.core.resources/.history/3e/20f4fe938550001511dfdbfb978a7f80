package com.maxiaohua.genealogy.main.db.tbl.support;

import com.maxiaohua.genealogy.fw.core.util.StringUtil;
import com.maxiaohua.genealogy.fw.core.util.DateUtil;
import com.maxiaohua.genealogy.main.db.base.BaseTableDTO;
import com.maxiaohua.genealogy.main.db.tbl.WashOrderBeyondDTO;

/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class WashOrderBeyondDTOSupport extends BaseTableDTO {

	public static final String INSERT = "WASH_ORDER_BEYOND_INSERT";

	public static final String FULL_UPDATE = "WASH_ORDER_BEYOND_FULL_UPDATE";

	public static final String UPDATE = "WASH_ORDER_BEYOND_UPDATE";

	public static final String DELETE = "WASH_ORDER_BEYOND_DELETE";

	public static final String FIND = "WASH_ORDER_BEYOND_FIND";

	public static final String FIND_FOR_UPDATE = "WASH_ORDER_BEYOND_FIND_FOR_UPDATE";

	public static final String EQUAL_SEARCH = "WASH_ORDER_BEYOND_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "WASH_ORDER_BEYOND_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "WASH_ORDER_BEYOND_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "WASH_ORDER_BEYOND_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "WASH_ORDER_BEYOND_SQL_SEARCH";

	public static final String COUNT = "WASH_ORDER_BEYOND_COUNT";

	public static final String COUNT_OR = "WASH_ORDER_BEYOND_COUNT_OR";

	public static final String COUNT_SEARCH = "WASH_ORDER_BEYOND_COUNT_SEARCH";

	public static final String COLUMN_BEYOND_ID = "BEYOND_ID";

	public static final String COLUMN_CITY_CODE = "CITY_CODE";

	public static final String COLUMN_CITY_AREA_ID = "CITY_AREA_ID";

	public static final String COLUMN_CUSTOMER_ID = "CUSTOMER_ID";

	public static final String COLUMN_CREATE_DATE = "CREATE_DATE";

	public static final String COLUMN_CREATE_TIME = "CREATE_TIME";

	public static final String COLUMN_REQUEST_DATE = "REQUEST_DATE";

	public static final String COLUMN_REQUEST_TIME = "REQUEST_TIME";

	public static final String COLUMN_LATITUDE = "LATITUDE";

	public static final String COLUMN_LONGITUDE = "LONGITUDE";

	public static final String TABLE = "WASH_ORDER_BEYOND";

	public static WashOrderBeyondDTO setDefaultValue() {
		WashOrderBeyondDTO defaultDTO = new WashOrderBeyondDTO();

		defaultDTO.setBeyondID(0l);

		defaultDTO.setCityCode(StringUtil.EMPTY);

		defaultDTO.setCityAreaID(0);

		defaultDTO.setCustomerID(0l);

		defaultDTO.setCreateDate(DateUtil.getDefaultDate());

		defaultDTO.setCreateTime(DateUtil.getDefaultTime());

		defaultDTO.setRequestDate(DateUtil.getDefaultDate());

		defaultDTO.setRequestTime(DateUtil.getDefaultTime());

		defaultDTO.setLatitude(0.0);

		defaultDTO.setLongitude(0.0);

		return defaultDTO;
	}
}
