package com.maxiaohua.genealogy.main.db.tbl.support;

import com.maxiaohua.genealogy.fw.core.util.StringUtil;
import com.maxiaohua.genealogy.main.db.base.BaseTableDTO;
import com.maxiaohua.genealogy.main.db.tbl.AreaKeywordDTO;

/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class AreaKeywordDTOSupport extends BaseTableDTO {

	public static final String INSERT = "AREA_KEYWORD_INSERT";

	public static final String FULL_UPDATE = "AREA_KEYWORD_FULL_UPDATE";

	public static final String UPDATE = "AREA_KEYWORD_UPDATE";

	public static final String DELETE = "AREA_KEYWORD_DELETE";

	public static final String FIND = "AREA_KEYWORD_FIND";

	public static final String FIND_FOR_UPDATE = "AREA_KEYWORD_FIND_FOR_UPDATE";

	public static final String EQUAL_SEARCH = "AREA_KEYWORD_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "AREA_KEYWORD_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "AREA_KEYWORD_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "AREA_KEYWORD_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "AREA_KEYWORD_SQL_SEARCH";

	public static final String COUNT = "AREA_KEYWORD_COUNT";

	public static final String COUNT_OR = "AREA_KEYWORD_COUNT_OR";

	public static final String COUNT_SEARCH = "AREA_KEYWORD_COUNT_SEARCH";

	public static final String COLUMN_AREA_KEYWORD_ID = "AREA_KEYWORD_ID";

	public static final String COLUMN_CITY_AREA_ID = "CITY_AREA_ID";

	public static final String COLUMN_KEYWORD = "KEYWORD";

	public static final String TABLE = "AREA_KEYWORD";

	public static AreaKeywordDTO setDefaultValue() {
		AreaKeywordDTO defaultDTO = new AreaKeywordDTO();

		defaultDTO.setAreaKeyWordID(0);

		defaultDTO.setCityAreaID(0);

		defaultDTO.setKeyWord(StringUtil.EMPTY);

		return defaultDTO;
	}
}
