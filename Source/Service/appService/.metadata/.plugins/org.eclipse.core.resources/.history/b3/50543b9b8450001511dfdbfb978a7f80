public abstract class AutomotiveManufacturerDTOSupport extends BaseTableDTO {

	public static final String INSERT = "AUTOMOTIVE_MANUFACTURER_INSERT";

	public static final String FULL_UPDATE = "AUTOMOTIVE_MANUFACTURER_FULL_UPDATE";

	public static final String UPDATE = "AUTOMOTIVE_MANUFACTURER_UPDATE";

	public static final String DELETE = "AUTOMOTIVE_MANUFACTURER_DELETE";

	public static final String FIND = "AUTOMOTIVE_MANUFACTURER_FIND";

	public static final String FIND_FOR_UPDATE = "AUTOMOTIVE_MANUFACTURER_FIND_FOR_UPDATE";

	public static final String EQUAL_SEARCH = "AUTOMOTIVE_MANUFACTURER_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "AUTOMOTIVE_MANUFACTURER_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "AUTOMOTIVE_MANUFACTURER_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "AUTOMOTIVE_MANUFACTURER_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "AUTOMOTIVE_MANUFACTURER_SQL_SEARCH";

