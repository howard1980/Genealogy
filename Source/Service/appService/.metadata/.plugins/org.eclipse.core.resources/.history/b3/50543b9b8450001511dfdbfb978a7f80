	public static AutomotiveManufacturerDTO setDefaultValue() {
		AutomotiveManufacturerDTO defaultDTO = new AutomotiveManufacturerDTO();

		defaultDTO.setAutomotiveManufacturerID(0l);

		defaultDTO.setAutomobileBrandID(0l);

