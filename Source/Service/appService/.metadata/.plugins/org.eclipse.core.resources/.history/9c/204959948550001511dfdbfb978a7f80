package com.maxiaohua.genealogy.main.db.tbl.support;

import com.maxiaohua.genealogy.fw.core.util.StringUtil;
import com.maxiaohua.genealogy.main.db.base.BaseTableDTO;
import com.maxiaohua.genealogy.main.db.tbl.AutomobileBrandDTO;

/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class AutomobileBrandDTOSupport extends BaseTableDTO {

	public static final String INSERT = "AUTOMOBILE_BRAND_INSERT";

	public static final String FULL_UPDATE = "AUTOMOBILE_BRAND_FULL_UPDATE";

	public static final String UPDATE = "AUTOMOBILE_BRAND_UPDATE";

	public static final String DELETE = "AUTOMOBILE_BRAND_DELETE";

	public static final String FIND = "AUTOMOBILE_BRAND_FIND";

	public static final String FIND_FOR_UPDATE = "AUTOMOBILE_BRAND_FIND_FOR_UPDATE";

	public static final String EQUAL_SEARCH = "AUTOMOBILE_BRAND_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "AUTOMOBILE_BRAND_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "AUTOMOBILE_BRAND_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "AUTOMOBILE_BRAND_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "AUTOMOBILE_BRAND_SQL_SEARCH";

	public static final String COUNT = "AUTOMOBILE_BRAND_COUNT";

	public static final String COUNT_OR = "AUTOMOBILE_BRAND_COUNT_OR";

	public static final String COUNT_SEARCH = "AUTOMOBILE_BRAND_COUNT_SEARCH";

	public static final String COLUMN_AUTOMOBILE_BRAND_ID = "AUTOMOBILE_BRAND_ID";

	public static final String COLUMN_NAME = "NAME";

	public static final String COLUMN_LOGO_PATH = "LOGO_PATH";

	public static final String COLUMN_SPELL = "SPELL";

	public static final String TABLE = "AUTOMOBILE_BRAND";

	public static AutomobileBrandDTO setDefaultValue() {
		AutomobileBrandDTO defaultDTO = new AutomobileBrandDTO();

		defaultDTO.setAutomobileBrandID(0l);

		defaultDTO.setNAME(StringUtil.EMPTY);

		defaultDTO.setLogoPath(StringUtil.EMPTY);

		defaultDTO.setSpell(StringUtil.EMPTY);

		return defaultDTO;
	}
}
