/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class RepairShopCarDTOSupport extends BaseTableDTO {

	public static final String INSERT = "REPAIR_SHOP_CAR_INSERT";

	public static final String FULL_UPDATE = "REPAIR_SHOP_CAR_FULL_UPDATE";

	public static final String UPDATE = "REPAIR_SHOP_CAR_UPDATE";

	public static final String DELETE = "REPAIR_SHOP_CAR_DELETE";

	public static final String FIND = "REPAIR_SHOP_CAR_FIND";

	public static final String FIND_FOR_UPDATE = "REPAIR_SHOP_CAR_FIND_FOR_UPDATE";

	public static final String EQUAL_SEARCH = "REPAIR_SHOP_CAR_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "REPAIR_SHOP_CAR_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "REPAIR_SHOP_CAR_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "REPAIR_SHOP_CAR_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "REPAIR_SHOP_CAR_SQL_SEARCH";

	public static final String COUNT = "REPAIR_SHOP_CAR_COUNT";

	public static final String COUNT_OR = "REPAIR_SHOP_CAR_COUNT_OR";

	public static final String COUNT_SEARCH = "REPAIR_SHOP_CAR_COUNT_SEARCH";

	public static final String COLUMN_REPAIR_SHOP_CAR_ID = "REPAIR_SHOP_CAR_ID";

	public static final String COLUMN_REPAIR_SHOP_ID = "REPAIR_SHOP_ID";

	public static final String COLUMN_AUTOMOTIVE_MANUFACTURER_ID = "AUTOMOTIVE_MANUFACTURER_ID";

	public static final String TABLE = "REPAIR_SHOP_CAR";

	public static RepairShopCarDTO setDefaultValue() {
		RepairShopCarDTO defaultDTO = new RepairShopCarDTO();

		defaultDTO.setRepairShopCarid(0l);

		defaultDTO.setRepairShopID(0l);

		defaultDTO.setAutomotiveManufacturerID(0l);

		return defaultDTO;
	}
}
