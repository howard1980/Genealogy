		this.price = price;
	}

	public Integer getamount() {
		return this.amount;
	}

	public void setamount(
			Integer amount) {
		this.amount = amount;
	}

  	/* (non-Javadoc)
     * @see java.lang.Comparable#compareTo(java.lang.Object)
     */
    @Override
    public int compareTo(
