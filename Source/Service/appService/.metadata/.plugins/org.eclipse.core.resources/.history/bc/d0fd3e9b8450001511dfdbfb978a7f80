package com.beetlestech.nfe.main.db.tbl.support;

import com.maxiaohua.genealogy.fw.core.util.StringUtil;
import com.beetlestech.nfe.main.db.base.BaseTableDTO;
import com.beetlestech.nfe.main.db.tbl.CarModelDTO;

/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class CarModelDTOSupport extends BaseTableDTO {

	public static final String INSERT = "CAR_MODEL_INSERT";

	public static final String FULL_UPDATE = "CAR_MODEL_FULL_UPDATE";

	public static final String UPDATE = "CAR_MODEL_UPDATE";

	public static final String DELETE = "CAR_MODEL_DELETE";

	public static final String FIND = "CAR_MODEL_FIND";

	public static final String FIND_FOR_UPDATE = "CAR_MODEL_FIND_FOR_UPDATE";

	public static final String EQUAL_SEARCH = "CAR_MODEL_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "CAR_MODEL_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "CAR_MODEL_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "CAR_MODEL_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "CAR_MODEL_SQL_SEARCH";

	public static final String COUNT = "CAR_MODEL_COUNT";

	public static final String COUNT_OR = "CAR_MODEL_COUNT_OR";

	public static final String COUNT_SEARCH = "CAR_MODEL_COUNT_SEARCH";

	public static final String COLUMN_CAR_MODEL_ID = "CAR_MODEL_ID";

	public static final String COLUMN_AUTOMOBILE_SERIES_ID = "AUTOMOBILE_SERIES_ID";

	public static final String COLUMN_NAME = "NAME";

	public static final String TABLE = "CAR_MODEL";

	public static CarModelDTO setDefaultValue() {
		CarModelDTO defaultDTO = new CarModelDTO();

		defaultDTO.setCarModelID(0l);

		defaultDTO.setAutomobileSeriesID(0l);

		defaultDTO.setName(StringUtil.EMPTY);

		return defaultDTO;
	}
}
