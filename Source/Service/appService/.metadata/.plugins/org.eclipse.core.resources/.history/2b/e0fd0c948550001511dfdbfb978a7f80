package com.maxiaohua.genealogy.main.db.tbl.support;

import com.maxiaohua.genealogy.main.db.base.BaseTableDTO;
import com.maxiaohua.genealogy.main.db.tbl.RepairShopProductDTO;

/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class RepairShopProductDTOSupport extends BaseTableDTO {

	public static final String INSERT = "REPAIR_SHOP_PRODUCT_INSERT";

	public static final String FULL_UPDATE = "REPAIR_SHOP_PRODUCT_FULL_UPDATE";

	public static final String UPDATE = "REPAIR_SHOP_PRODUCT_UPDATE";

	public static final String DELETE = "REPAIR_SHOP_PRODUCT_DELETE";

	public static final String FIND = "REPAIR_SHOP_PRODUCT_FIND";

	public static final String FIND_FOR_UPDATE = "REPAIR_SHOP_PRODUCT_FIND_FOR_UPDATE";

	public static final String EQUAL_SEARCH = "REPAIR_SHOP_PRODUCT_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "REPAIR_SHOP_PRODUCT_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "REPAIR_SHOP_PRODUCT_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "REPAIR_SHOP_PRODUCT_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "REPAIR_SHOP_PRODUCT_SQL_SEARCH";

	public static final String COUNT = "REPAIR_SHOP_PRODUCT_COUNT";

	public static final String COUNT_OR = "REPAIR_SHOP_PRODUCT_COUNT_OR";

	public static final String COUNT_SEARCH = "REPAIR_SHOP_PRODUCT_COUNT_SEARCH";

	public static final String COLUMN_SHOP_SERVICE_ID = "SHOP_SERVICE_ID";

	public static final String COLUMN_REPAIR_SHOP_ID = "REPAIR_SHOP_ID";

	public static final String COLUMN_SERVICE_PRODUCT_ID = "SERVICE_PRODUCT_ID";

	public static final String TABLE = "REPAIR_SHOP_PRODUCT";

	public static RepairShopProductDTO setDefaultValue() {
		RepairShopProductDTO defaultDTO = new RepairShopProductDTO();

		defaultDTO.setShopServiceID(0l);

		defaultDTO.setRepairShopID(0l);

		defaultDTO.setServiceProductID(0l);

		return defaultDTO;
	}
}
