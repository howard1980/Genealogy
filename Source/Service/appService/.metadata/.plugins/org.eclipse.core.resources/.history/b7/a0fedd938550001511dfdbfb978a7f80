 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class AdminUserGroupDTOSupport extends BaseTableDTO {

	public static final String INSERT = "ADMIN_USER_GROUP_INSERT";

	public static final String FULL_UPDATE = "ADMIN_USER_GROUP_FULL_UPDATE";

	public static final String UPDATE = "ADMIN_USER_GROUP_UPDATE";

	public static final String DELETE = "ADMIN_USER_GROUP_DELETE";

	public static final String FIND = "ADMIN_USER_GROUP_FIND";

	public static final String FIND_FOR_UPDATE = "ADMIN_USER_GROUP_FIND_FOR_UPDATE";

	public static final String EQUAL_SEARCH = "ADMIN_USER_GROUP_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "ADMIN_USER_GROUP_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "ADMIN_USER_GROUP_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "ADMIN_USER_GROUP_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "ADMIN_USER_GROUP_SQL_SEARCH";

	public static final String COUNT = "ADMIN_USER_GROUP_COUNT";

	public static final String COUNT_OR = "ADMIN_USER_GROUP_COUNT_OR";

	public static final String COUNT_SEARCH = "ADMIN_USER_GROUP_COUNT_SEARCH";

	public static final String COLUMN_ADMINISTRATOR_ID = "ADMINISTRATOR_ID";

	public static final String COLUMN_GROUP_ID = "GROUP_ID";

	public static final String TABLE = "ADMIN_USER_GROUP";

	public static AdminUserGroupDTO setDefaultValue() {
		AdminUserGroupDTO defaultDTO = new AdminUserGroupDTO();

		defaultDTO.setadministratorid(0l);

		defaultDTO.setgroupid(0l);

		return defaultDTO;
	}
}
