package com.maxiaohua.genealogy.main.db.tbl.support;

import com.maxiaohua.genealogy.main.db.base.BaseTableDTO;
import com.maxiaohua.genealogy.main.db.tbl.AreaInfoDTO;

/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class AreaInfoDTOSupport extends BaseTableDTO {

	public static final String INSERT = "AREA_INFO_INSERT";

	public static final String FULL_UPDATE = "AREA_INFO_FULL_UPDATE";

	public static final String UPDATE = "AREA_INFO_UPDATE";

	public static final String DELETE = "AREA_INFO_DELETE";

	public static final String FIND = "AREA_INFO_FIND";

	public static final String FIND_FOR_UPDATE = "AREA_INFO_FIND_FOR_UPDATE";

	public static final String EQUAL_SEARCH = "AREA_INFO_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "AREA_INFO_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "AREA_INFO_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "AREA_INFO_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "AREA_INFO_SQL_SEARCH";

	public static final String COUNT = "AREA_INFO_COUNT";

	public static final String COUNT_OR = "AREA_INFO_COUNT_OR";

	public static final String COUNT_SEARCH = "AREA_INFO_COUNT_SEARCH";

	public static final String COLUMN_AREA_INFO_ID = "AREA_INFO_ID";

	public static final String COLUMN_CITY_AREA_ID = "CITY_AREA_ID";

	public static final String COLUMN_SORT = "SORT";

	public static final String COLUMN_LONGITUDE = "LONGITUDE";

	public static final String COLUMN_LATITUDE = "LATITUDE";

	public static final String TABLE = "AREA_INFO";

	public static AreaInfoDTO setDefaultValue() {
		AreaInfoDTO defaultDTO = new AreaInfoDTO();

		defaultDTO.setAreaInfoID(0);

		defaultDTO.setCityAreaID(0);

		defaultDTO.setSort(0);

		defaultDTO.setLongitude(0.0);

		defaultDTO.setLatitude(0.0);

		return defaultDTO;
	}
}
