package com.beetlestech.nfe.main.db.tbl.support;

import com.maxiaohua.genealogy.fw.core.util.StringUtil;
import com.beetlestech.nfe.main.db.base.BaseTableDTO;
import com.beetlestech.nfe.main.db.tbl.CommonDTO;

/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class CommonDTOSupport extends BaseTableDTO {

	public static final String INSERT = "COMMON_INSERT";

	public static final String FULL_UPDATE = "COMMON_FULL_UPDATE";

	public static final String UPDATE = "COMMON_UPDATE";

	public static final String DELETE = "COMMON_DELETE";

	public static final String FIND = "COMMON_FIND";

	public static final String FIND_FOR_UPDATE = "COMMON_FIND_FOR_UPDATE";

	public static final String EQUAL_SEARCH = "COMMON_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "COMMON_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "COMMON_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "COMMON_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "COMMON_SQL_SEARCH";

	public static final String COUNT = "COMMON_COUNT";

	public static final String COUNT_OR = "COMMON_COUNT_OR";

	public static final String COUNT_SEARCH = "COMMON_COUNT_SEARCH";

	public static final String COLUMN_KBN = "KBN";

	public static final String COLUMN_CODE1 = "CODE1";

	public static final String COLUMN_CODE2 = "CODE2";

	public static final String COLUMN_CODE3 = "CODE3";

	public static final String COLUMN_VALUE = "VALUE";

	public static final String COLUMN_REMARKS = "REMARKS";

	public static final String TABLE = "COMMON";

	public static CommonDTO setDefaultValue() {
		CommonDTO defaultDTO = new CommonDTO();

		defaultDTO.setKBN(StringUtil.EMPTY);

		defaultDTO.setCode1(StringUtil.EMPTY);

		defaultDTO.setCode2(StringUtil.EMPTY);

		defaultDTO.setCode3(StringUtil.EMPTY);

		defaultDTO.setValue(StringUtil.EMPTY);

		defaultDTO.setRemarks(StringUtil.EMPTY);

		return defaultDTO;
	}
}
