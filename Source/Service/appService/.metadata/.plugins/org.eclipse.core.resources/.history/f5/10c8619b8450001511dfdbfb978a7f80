package com.beetlestech.nfe.main.db.tbl.support;

import com.maxiaohua.genealogy.fw.core.util.StringUtil;
import com.maxiaohua.genealogy.fw.core.util.DateUtil;
import com.beetlestech.nfe.main.db.base.BaseTableDTO;
import com.beetlestech.nfe.main.db.tbl.MallOrderDTO;

/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class MallOrderDTOSupport extends BaseTableDTO {

	public static final String INSERT = "MALL_ORDER_INSERT";

	public static final String FULL_UPDATE = "MALL_ORDER_FULL_UPDATE";

	public static final String UPDATE = "MALL_ORDER_UPDATE";

	public static final String DELETE = "MALL_ORDER_DELETE";

	public static final String FIND = "MALL_ORDER_FIND";

	public static final String FIND_FOR_UPDATE = "MALL_ORDER_FIND_FOR_UPDATE";

	public static final String EQUAL_SEARCH = "MALL_ORDER_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "MALL_ORDER_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "MALL_ORDER_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "MALL_ORDER_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "MALL_ORDER_SQL_SEARCH";

	public static final String COUNT = "MALL_ORDER_COUNT";

	public static final String COUNT_OR = "MALL_ORDER_COUNT_OR";

	public static final String COUNT_SEARCH = "MALL_ORDER_COUNT_SEARCH";

	public static final String COLUMN_MALL_ORDER_ID = "MALL_ORDER_ID";

	public static final String COLUMN_CUSTOMER_ID = "CUSTOMER_ID";

	public static final String COLUMN_SHOP_ORDER_ID = "SHOP_ORDER_ID";

	public static final String COLUMN_AMMOUNT = "AMMOUNT";

	public static final String COLUMN_PAYTYPE = "PAYTYPE";

	public static final String COLUMN_EXPLAIN = "EXPLAIN";

	public static final String COLUMN_CREATE_DATE = "CREATE_DATE";

	public static final String COLUMN_CREATE_TIME = "CREATE_TIME";

	public static final String TABLE = "MALL_ORDER";

	public static MallOrderDTO setDefaultValue() {
		MallOrderDTO defaultDTO = new MallOrderDTO();

		defaultDTO.setMallOrderID(0l);

		defaultDTO.setCustomerID(0l);

		defaultDTO.setShopOrderID(StringUtil.EMPTY);

		defaultDTO.setAmmount(new java.math.BigDecimal(0));

		defaultDTO.setPayType(0);

		defaultDTO.setExplain(StringUtil.EMPTY);

		defaultDTO.setCreateDate(DateUtil.getDefaultDate());

		defaultDTO.setCreateTime(DateUtil.getDefaultTime());

		return defaultDTO;
	}
}
