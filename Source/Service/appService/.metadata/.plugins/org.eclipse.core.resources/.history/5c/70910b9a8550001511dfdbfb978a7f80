package com.maxiaohua.genealogy.main.db.view.support;

import com.maxiaohua.genealogy.main.db.base.BaseTableDTO;
import com.maxiaohua.genealogy.main.db.view.VCustomerAccountDTO;

/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class VCustomerAccountDTOSupport extends BaseTableDTO {

	public static final String EQUAL_SEARCH = "VCUSTOMER_ACCOUNT_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "VCUSTOMER_ACCOUNT_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "VCUSTOMER_ACCOUNT_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "VCUSTOMER_ACCOUNT_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "VCUSTOMER_ACCOUNT_SQL_SEARCH";

	public static final String COUNT = "VCUSTOMER_ACCOUNT_COUNT";

	public static final String COUNT_OR = "VCUSTOMER_ACCOUNT_COUNT_OR";

	public static final String COUNT_SEARCH = "VCUSTOMER_ACCOUNT_COUNT_SEARCH";

	public static final String COLUMN_CUSTOMER_ID = "CUSTOMER_ID";

	public static final String COLUMN_OLDUID = "OLDUID";

	public static final String COLUMN_RECHARGEAMOUNT = "RECHARGEAMOUNT";

	public static final String COLUMN_PAYAMOUNT = "PAYAMOUNT";

	public static final String TABLE = "VCUSTOMER_ACCOUNT";

	public static VCustomerAccountDTO setDefaultValue() {
		VCustomerAccountDTO defaultDTO = new VCustomerAccountDTO();

		defaultDTO.setCustomerID(0l);

		defaultDTO.setOldUID(0l);

		defaultDTO.setRechargeAmount(new java.math.BigDecimal(0));

		defaultDTO.setPayAmount(new java.math.BigDecimal(0));

		return defaultDTO;
	}
}
