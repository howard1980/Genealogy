package com.maxiaohua.genealogy.main.db.tbl.support;

import com.maxiaohua.genealogy.main.db.base.BaseTableDTO;
import com.maxiaohua.genealogy.main.db.tbl.RoleFunctionsDTO;

/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class RoleFunctionsDTOSupport extends BaseTableDTO {

	public static final String INSERT = "ROLE_FUNCTIONS_INSERT";

	public static final String FULL_UPDATE = "ROLE_FUNCTIONS_FULL_UPDATE";

	public static final String UPDATE = "ROLE_FUNCTIONS_UPDATE";

	public static final String DELETE = "ROLE_FUNCTIONS_DELETE";

	public static final String FIND = "ROLE_FUNCTIONS_FIND";

	public static final String FIND_FOR_UPDATE = "ROLE_FUNCTIONS_FIND_FOR_UPDATE";

	public static final String EQUAL_SEARCH = "ROLE_FUNCTIONS_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "ROLE_FUNCTIONS_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "ROLE_FUNCTIONS_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "ROLE_FUNCTIONS_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "ROLE_FUNCTIONS_SQL_SEARCH";

	public static final String COUNT = "ROLE_FUNCTIONS_COUNT";

	public static final String COUNT_OR = "ROLE_FUNCTIONS_COUNT_OR";

	public static final String COUNT_SEARCH = "ROLE_FUNCTIONS_COUNT_SEARCH";

	public static final String COLUMN_ROLE_ID = "ROLE_ID";

	public static final String COLUMN_FUNCTIONS_MODULES_ID = "FUNCTIONS_MODULES_ID";

	public static final String TABLE = "ROLE_FUNCTIONS";

	public static RoleFunctionsDTO setDefaultValue() {
		RoleFunctionsDTO defaultDTO = new RoleFunctionsDTO();

		defaultDTO.setroleid(0l);

		defaultDTO.setfunctionsmodulesid(0l);

		return defaultDTO;
	}
}
