package com.maxiaohua.genealogy.main.db.view.support;

import com.maxiaohua.genealogy.main.db.base.BaseTableDTO;
import com.maxiaohua.genealogy.main.db.view.VOrderAmmountDTO;

/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class VOrderAmmountDTOSupport extends BaseTableDTO {

	public static final String EQUAL_SEARCH = "VORDER_AMMOUNT_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "VORDER_AMMOUNT_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "VORDER_AMMOUNT_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "VORDER_AMMOUNT_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "VORDER_AMMOUNT_SQL_SEARCH";

	public static final String COUNT = "VORDER_AMMOUNT_COUNT";

	public static final String COUNT_OR = "VORDER_AMMOUNT_COUNT_OR";

	public static final String COUNT_SEARCH = "VORDER_AMMOUNT_COUNT_SEARCH";

	public static final String COLUMN_WASH_ORDER_ID = "WASH_ORDER_ID";

	public static final String COLUMN_CAR_WASH_ID = "CAR_WASH_ID";

	public static final String COLUMN_AMMOUNT = "AMMOUNT";

	public static final String COLUMN_PAYMENT = "PAYMENT";

	public static final String COLUMN_STATUS = "STATUS";

	public static final String TABLE = "VORDER_AMMOUNT";

	public static VOrderAmmountDTO setDefaultValue() {
		VOrderAmmountDTO defaultDTO = new VOrderAmmountDTO();

		defaultDTO.setWashOrderID(0l);

		defaultDTO.setCarWashID(0l);

		defaultDTO.setAmmount(new java.math.BigDecimal(0));

		defaultDTO.setPayment(new java.math.BigDecimal(0));

		defaultDTO.setStatus(0);

		return defaultDTO;
	}
}
