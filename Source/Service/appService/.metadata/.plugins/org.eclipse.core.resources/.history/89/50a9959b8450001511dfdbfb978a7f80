package com.beetlestech.nfe.main.db.tbl.support;

import com.maxiaohua.genealogy.fw.core.util.StringUtil;
import com.beetlestech.nfe.main.db.base.BaseTableDTO;
import com.beetlestech.nfe.main.db.tbl.WashItemBeyondDTO;

/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class WashItemBeyondDTOSupport extends BaseTableDTO {

	public static final String INSERT = "WASH_ITEM_BEYOND_INSERT";

	public static final String FULL_UPDATE = "WASH_ITEM_BEYOND_FULL_UPDATE";

	public static final String UPDATE = "WASH_ITEM_BEYOND_UPDATE";

	public static final String DELETE = "WASH_ITEM_BEYOND_DELETE";

	public static final String FIND = "WASH_ITEM_BEYOND_FIND";

	public static final String FIND_FOR_UPDATE = "WASH_ITEM_BEYOND_FIND_FOR_UPDATE";

	public static final String EQUAL_SEARCH = "WASH_ITEM_BEYOND_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "WASH_ITEM_BEYOND_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "WASH_ITEM_BEYOND_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "WASH_ITEM_BEYOND_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "WASH_ITEM_BEYOND_SQL_SEARCH";

	public static final String COUNT = "WASH_ITEM_BEYOND_COUNT";

	public static final String COUNT_OR = "WASH_ITEM_BEYOND_COUNT_OR";

	public static final String COUNT_SEARCH = "WASH_ITEM_BEYOND_COUNT_SEARCH";

	public static final String COLUMN_ITEM_BEYOND_ID = "ITEM_BEYOND_ID";

	public static final String COLUMN_BEYOND_ID = "BEYOND_ID";

	public static final String COLUMN_SERVICE_ITEMS_ID = "SERVICE_ITEMS_ID";

	public static final String COLUMN_SERVICE_COUNT = "SERVICE_COUNT";

	public static final String TABLE = "WASH_ITEM_BEYOND";

	public static WashItemBeyondDTO setDefaultValue() {
		WashItemBeyondDTO defaultDTO = new WashItemBeyondDTO();

		defaultDTO.setItemBeyondID(0l);

		defaultDTO.setBeyondID(0l);

		defaultDTO.setServiceItemsID(StringUtil.EMPTY);

		defaultDTO.setServiceCount(0);

		return defaultDTO;
	}
}
