package com.maxiaohua.genealogy.main.db.tbl.support;

import com.maxiaohua.genealogy.main.db.base.BaseTableDTO;
import com.maxiaohua.genealogy.main.db.tbl.HistoryAccountDTO;

/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class HistoryAccountDTOSupport extends BaseTableDTO {

	public static final String INSERT = "HISTORY_ACCOUNT_INSERT";

	public static final String FULL_UPDATE = "HISTORY_ACCOUNT_FULL_UPDATE";

	public static final String UPDATE = "HISTORY_ACCOUNT_UPDATE";

	public static final String DELETE = "HISTORY_ACCOUNT_DELETE";

	public static final String FIND = "HISTORY_ACCOUNT_FIND";

	public static final String FIND_FOR_UPDATE = "HISTORY_ACCOUNT_FIND_FOR_UPDATE";

	public static final String EQUAL_SEARCH = "HISTORY_ACCOUNT_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "HISTORY_ACCOUNT_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "HISTORY_ACCOUNT_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "HISTORY_ACCOUNT_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "HISTORY_ACCOUNT_SQL_SEARCH";

	public static final String COUNT = "HISTORY_ACCOUNT_COUNT";

	public static final String COUNT_OR = "HISTORY_ACCOUNT_COUNT_OR";

	public static final String COUNT_SEARCH = "HISTORY_ACCOUNT_COUNT_SEARCH";

	public static final String COLUMN_HISTORY_ACCOUNT_ID = "HISTORY_ACCOUNT_ID";

	public static final String COLUMN_CUSTOMER_ID = "CUSTOMER_ID";

	public static final String COLUMN_NEW_AMOUNT = "NEW_AMOUNT";

	public static final String COLUMN_OLD_AMOUNT = "OLD_AMOUNT";

	public static final String TABLE = "HISTORY_ACCOUNT";

	public static HistoryAccountDTO setDefaultValue() {
		HistoryAccountDTO defaultDTO = new HistoryAccountDTO();

		defaultDTO.setHistoryAccountID(0l);

		defaultDTO.setCustomerID(0l);

		defaultDTO.setNewAmount(new java.math.BigDecimal(0));

		defaultDTO.setOldAmount(new java.math.BigDecimal(0));

		return defaultDTO;
	}
}
