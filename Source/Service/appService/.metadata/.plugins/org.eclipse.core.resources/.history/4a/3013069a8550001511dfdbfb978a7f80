import com.maxiaohua.genealogy.fw.core.util.StringUtil;
import com.maxiaohua.genealogy.main.db.base.BaseTableDTO;
import com.maxiaohua.genealogy.main.db.view.VCityAreaDTO;

/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class VCityAreaDTOSupport extends BaseTableDTO {

	public static final String EQUAL_SEARCH = "VCITY_AREA_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "VCITY_AREA_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "VCITY_AREA_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "VCITY_AREA_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "VCITY_AREA_SQL_SEARCH";

	public static final String COUNT = "VCITY_AREA_COUNT";

	public static final String COUNT_OR = "VCITY_AREA_COUNT_OR";

	public static final String COUNT_SEARCH = "VCITY_AREA_COUNT_SEARCH";

	public static final String COLUMN_CITY_CODE = "CITY_CODE";

	public static final String COLUMN_CENTER_LATITUDUE = "CENTER_LATITUDUE";

	public static final String COLUMN_CENTER_LONGITUDE = "CENTER_LONGITUDE";

	public static final String COLUMN_CITY_AREA_ID = "CITY_AREA_ID";

	public static final String COLUMN_WORKER_GROUP_ID = "WORKER_GROUP_ID";

	public static final String COLUMN_OPENFLG = "OPENFLG";

	public static final String COLUMN_NEED = "NEED";

	public static final String COLUMN_CEILING = "CEILING";

	public static final String COLUMN_ORDER_COUNT = "ORDER_COUNT";

	public static final String COLUMN_LONGITUDE = "LONGITUDE";

	public static final String COLUMN_LATITUDE = "LATITUDE";

	public static final String COLUMN_SORT = "SORT";

	public static final String TABLE = "VCITY_AREA";

	public static VCityAreaDTO setDefaultValue() {
		VCityAreaDTO defaultDTO = new VCityAreaDTO();

		defaultDTO.setcitycode(StringUtil.EMPTY);

		defaultDTO.setCenterLatitudue(0.0);

		defaultDTO.setCenterLongitude(0.0);

		defaultDTO.setCityAreaID(0);

		defaultDTO.setWorkerGroupID(0);

		defaultDTO.setOpenFlg(0);

		defaultDTO.setNeed(0);

		defaultDTO.setCeiling(0);

		defaultDTO.setOrderCount(0);

		defaultDTO.setLongitude(0.0);

		defaultDTO.setLatitude(0.0);

		defaultDTO.setSort(0);

		return defaultDTO;
	}
}
