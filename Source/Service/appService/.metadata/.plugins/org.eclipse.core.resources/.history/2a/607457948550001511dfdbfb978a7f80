package com.maxiaohua.genealogy.main.db.tbl.support;

import com.maxiaohua.genealogy.main.db.base.BaseTableDTO;
import com.maxiaohua.genealogy.main.db.tbl.ServiceMappingDTO;

/**
 *Service DB Support DTO
 *
 * History
 * REV.         Updated Date           Updater              Infomation
 * -------      ---------------        ----------------     ------------------
 * 1.0          2015/05/25             TOOL                 Create
*
 */
public abstract class ServiceMappingDTOSupport extends BaseTableDTO {

	public static final String INSERT = "SERVICE_MAPPING_INSERT";

	public static final String FULL_UPDATE = "SERVICE_MAPPING_FULL_UPDATE";

	public static final String UPDATE = "SERVICE_MAPPING_UPDATE";

	public static final String DELETE = "SERVICE_MAPPING_DELETE";

	public static final String FIND = "SERVICE_MAPPING_FIND";

	public static final String FIND_FOR_UPDATE = "SERVICE_MAPPING_FIND_FOR_UPDATE";

	public static final String EQUAL_SEARCH = "SERVICE_MAPPING_EQUAL_SEARCH";

	public static final String EQUAL_SEARCH_OR = "SERVICE_MAPPING_EQUAL_SEARCH_OR";

	public static final String LIKE_SEARCH = "SERVICE_MAPPING_LIKE_SEARCH";

	public static final String LIKE_SEARCH_OR = "SERVICE_MAPPING_LIKE_SEARCH_OR";

	public static final String SQL_SEARCH = "SERVICE_MAPPING_SQL_SEARCH";

	public static final String COUNT = "SERVICE_MAPPING_COUNT";

	public static final String COUNT_OR = "SERVICE_MAPPING_COUNT_OR";

	public static final String COUNT_SEARCH = "SERVICE_MAPPING_COUNT_SEARCH";

	public static final String COLUMN_SERVICE_MAPPING_ID = "SERVICE_MAPPING_ID";

	public static final String COLUMN_SERVICE_PRODUCT_ID = "SERVICE_PRODUCT_ID";

	public static final String COLUMN_REPAIR_SERVICE_ID = "REPAIR_SERVICE_ID";

	public static final String TABLE = "SERVICE_MAPPING";

	public static ServiceMappingDTO setDefaultValue() {
		ServiceMappingDTO defaultDTO = new ServiceMappingDTO();

		defaultDTO.setServiceMappingID(0l);

		defaultDTO.setServiceProductID(0l);

		defaultDTO.setRepairServiceID(0l);

		return defaultDTO;
	}
}
